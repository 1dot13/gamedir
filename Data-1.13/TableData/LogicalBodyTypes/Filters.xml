			<WEAPON_TYPE>GUN_PISTOL</WEAPON_TYPE>
		</AND>
		<OR>
			<LEFT_WEAPON_TYPE>GUN_PISTOL</LEFT_WEAPON_TYPE>
			<LEFT_WEAPON_TYPE>GUN_M_PISTOL</LEFT_WEAPON_TYPE>
